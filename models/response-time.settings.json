{
  "kind": "collectionType",
  "collectionName": "response-times",
  "info": {
    "name": "Response Time"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false,
    "populateCreatorFields": false
  },
  "attributes": {
    "url": {
      "type": "string",
      "required": true
    },
    "method": {
      "type": "string",
      "required": false
    },
    "responseTime": {
      "type": "integer",
      "required": false
    },
    "status": {
      "type": "integer",
      "required": false
    },
    "error": {
      "type": "boolean",
      "required": false
    },
    "throttled": {
      "type": "boolean",
      "required": false,
      "default": false
    }
  }
}